{
  "manifest_version": 3,

  "name": "Shelf",
  "description": "An easier way to re-open your tabs",
  "version": "2.0.0",

  "action": {
    "default_popup": "index.html"
    
  },


  "icons": {
    "16": "./logo.png",
    "48": "./logo.png",
    "128": "./logo.png"
  },
  "permissions": [
    "storage", "tabs", "activeTab"
  ]
}
